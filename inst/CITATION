citHeader("To cite idmodelr in publications use:")

citEntry(entry = "Article",
  title        = "idmodelr",
  author       = personList(as.person("Sam Abbott")),
  journal      = "*",
  year         = "2019",
  volume       = "*",
  number       = "*",
  pages        = "*",
  doi          = "10.5281/zenodo.2641048",

  textVersion  =
  paste("Sam Abbott (2019).",
        "idmodelr,",
        "DOI: 10.5281/zenodo.2641048")
)
